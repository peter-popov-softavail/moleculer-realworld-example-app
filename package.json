{
  "name": "moleculer-realworld-example-app",
  "version": "1.0.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "4.11.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "moleculer-repl": "^0.6.5",
    "npm-check": "^5.9.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.13",
    "moleculer-db": "^0.8.14",
    "moleculer-db-adapter-mongo": "^0.4.12",
    "moleculer-web": "^0.10.0-beta2",
    "nats": "^2.0.4",
    "slug": "^5.0.1",
    "@moleculer/lab": "^0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ice-services/moleculer-realworld-example-app.git"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
